{
  "routes": [
    {
      "route": "/assets/*",
      "headers": {
        "cache-control": "must-revalidate, max-age=15770000"
      }
    },
    {
      "route": "/*",
      "headers": {
        "cache-control": "must-revalidate, max-age=600"
      }
    },
    {
      "route": "/index.html",
      "headers": {
        "cache-control": "no-cache"
      }
    },
    {
      "route": "/*",
      "rewrite": "/index.html"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*.{png,jpg,gif}", "/css/*", "/assets/*"]
  }
}
